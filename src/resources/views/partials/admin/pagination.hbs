{{#ifNotEquals pagination.pageCount 1}}
    <nav aria-label="Page navigation example">
        <ul class="pagination pagination-primary  justify-content-end">
            {{#ifNotEquals pagination.currentPage 1}}
                <li class="page-item">
                    <a class="page-link" href="{{ pagination.previousHref }}" tabindex="-1" aria-disabled="true">Previous</a>
                </li>
            {{/ifNotEquals}}
            {{#each pagination.pageItem}}
                {{#if this.active}}
                    <li class="page-item active"><a class="page-link" href="{{this.pageHref}}">{{this.index}}</a></li>
                {{else}}
                    <li class="page-item"><a class="page-link" href="{{this.pageHref}}">{{this.index}}</a></li>
                {{/if}}
            {{/each}}
            {{#ifNotEquals pagination.currentPage pagination.pageCount}}
                <li class="page-item">
                    <a class="page-link" href="{{pagination.nextHref}}">Next</a>
                </li>
            {{/ifNotEquals}}
        </ul>
    </nav>
{{/ifNotEquals}}